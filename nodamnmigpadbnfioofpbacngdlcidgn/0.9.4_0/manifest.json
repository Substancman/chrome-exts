{
   "author": "Blue Jeans Network",
   "background": {
      "scripts": [ "js/background-scripts/carmelLauncher.js", "js/background-scripts/RTCDesktopMediaManager.js", "js/background-scripts/backgroundMain.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/content-scripts/content_script.js" ],
      "matches": [ "*://*.bluejeans.com/*", "*://*.bluejeansnet.com/*", "*://*.bluejeansdev.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "This extension enhances the Blue Jeans experience for Chrome users.",
   "externally_connectable": {
      "matches": [ "*://*.bluejeans.com/*", "*://*.bluejeansnet.com/*", "*://*.bluejeansdev.com/*" ]
   },
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Blue Jeans Meeting",
   "permissions": [ "nativeMessaging", "desktopCapture" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.9.4"
}
