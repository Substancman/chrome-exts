{
   "app": {
      "background": {
         "scripts": [ "chrome-main.js" ]
      }
   },
   "author": "Jaros≈Çaw Foksa",
   "description": "Scalable Vector Graphics (SVG) editor for Chrome OS",
   "file_handlers": {
      "gif": {
         "extensions": [ "gif" ]
      },
      "jpeg": {
         "extensions": [ "jpg", "jpeg" ]
      },
      "png": {
         "extensions": [ "png" ]
      },
      "svg": {
         "extensions": [ "svg" ]
      },
      "svgz": {
         "extensions": [ "svgz" ]
      },
      "webp": {
         "extensions": [ "webp" ]
      }
   },
   "icons": {
      "128": "images/app-128.png",
      "16": "images/app-16.png",
      "32": "images/app-32.png",
      "48": "images/app-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "58",
   "name": "Boxy SVG",
   "oauth2": {
      "client_id": "166009762809-enl73avab54786ngilubh4jcidnmr947.apps.googleusercontent.com",
      "scopes": [ "https://www.googleapis.com/auth/chromewebstore.readonly" ]
   },
   "offline_enabled": true,
   "permissions": [ "browser", "webview", "storage", "identity", "https://www.googleapis.com/", "clipboardRead", "clipboardWrite", "idle", {
      "fileSystem": [ "write", "retainEntries" ]
   } ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.10.2",
   "webview": {
      "partitions": [ {
         "accessible_resources": [ "*" ],
         "name": "boxy"
      } ]
   }
}
