{
   "background": {
      "scripts": [ "js/JsonValues.min.js", "js/background.min.js", "js/bg.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Responsive Web Design Tester"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self';",
   "description": "Responsive Web Design Tester is a quick and easy way to test your responsive website.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Responsive Web Design Tester",
   "offline_enabled": true,
   "options_page": "options.html",
   "permissions": [ "webRequest", "webRequestBlocking", "webNavigation", "tabs", "http://*/*", "https://*/*", "contextMenus", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.24"
}
